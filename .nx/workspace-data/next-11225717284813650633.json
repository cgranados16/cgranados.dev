{
  "7681945387616864397": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/portfolio",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/portfolio/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/portfolio/.next/!(cache)"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/portfolio"
      }
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/portfolio"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/portfolio"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "pnpm exec nx watch --projects portfolio --includeDependentProjects -- pnpm exec nx build-deps portfolio"
    }
  },
