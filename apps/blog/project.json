{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "blog",
  "tags": ["scope:blog", "type:app"],
  "projectType": "application",
  "sourceRoot": "apps/blog/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/blog",
        "command": "astro build"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/blog",
        "command": "astro dev"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/blog",
        "command": "astro preview"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/blog",
        "command": "astro check"
      }
    },
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/blog",
        "command": "astro sync"
      }
    }
  }
}
